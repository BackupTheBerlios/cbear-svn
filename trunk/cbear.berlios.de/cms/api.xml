<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../api/html.xsl"?>
<library 
	xmlns="http://cbear.berlios.de/api"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://cbear.berlios.de/api ../api/main.xsd"
	xmlns:html="http://www.w3.org/1999/xhtml"
	id="net.sourceforge.cbear.project">

<using id="Base" href="../api/base.xml"/>

<union id="Context">
	<object id="para">
		<type.ref library="Base" id="string"/>
	</object>
</union>

<struct id="Project">

	<type.ref library="Base" id="vector">
		<type.ref id="Project"/>
	</type.ref>

	<type.ref library="Base" id="vector">
		<type.ref id="Context"/>
	</type.ref>

	<object id="name">
		<pragma><xsd:attribute/></pragma>
		<type.ref library="Base" id="string"/>
	</object>

	<object id="id">
		<pragma><xsd:attribute/></pragma>
		<type.ref library="Base" id="optional">
			<type.ref library="Base" id="string"/>
		</type.ref>
	</object>

</struct>

</library>
