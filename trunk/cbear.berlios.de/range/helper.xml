<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../cpp/html.xsl"?>
<unit 
	xmlns="http://cbear.berlios.de/cpp" 
	id="cbear.berlios.de/range/helper"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://cbear.berlios.de/cpp ../cpp/main.xsd">
<header>
<namespace id="cbear_berlios_de">
<namespace id="range">

<template>
	<id id="Container"/>
	<id id="Iterator"/>
	<id id="ConstIterator"/>
	<class>
		<id.ref id="helper"/>
		<access access="public">
			<typedef id="container"><id.ref id="Container"/></typedef>
			<typedef id="iterator"><id.ref id="Iterator"/></typedef>
			<typedef id="const_iterator"><id.ref id="ConstIterator"/></typedef>
			<typedef id="value_type"><id.ref id="..."/></typedef>
			<typedef id="difference_type"><id.ref id="..."/></typedef>
			<typedef id="size_type"><id.ref id="..."/></typedef>
			<typedef id="reference"><id.ref id="..."/></typedef>
			<typedef id="const_reference"><id.ref id="..."/></typedef>
			<typedef id="pointer"><id.ref id="..."/></typedef>
			<typedef id="const_pointer"><id.ref id="..."/></typedef>
			<typedef id="reverse_iterator"><id.ref id="..."/></typedef>
			<typedef id="const_reverse_iterator"><id.ref id="..."/></typedef>
			<method id="empty">
				<id.ref id="bool"/>
				<const/>
			</method>
			<method id="size">
				<id.ref id="size_type"/>
				<const/>
			</method>
			<method id="front">
				<id.ref id="reference"/>
			</method>
			<method id="front">
				<id.ref id="const_reference"/>
				<const/>
			</method>
			<method id="back">
				<id.ref id="reference"/>
			</method>
			<method id="back">
				<id.ref id="const_reference"/>
				<const/>
			</method>
			<method id="[]" type="operator">
				<id.ref id="reference"/>
				<parameter><id.ref id="size_type"/></parameter>
			</method>
			<method id="[]" type="operator">
				<id.ref id="const_reference"/>
				<parameter><id.ref id="size_type"/></parameter>
				<const/>
			</method>
			<method id="rbegin">
				<id.ref id="reverse_iterator"/>
			</method>
			<method id="rbegin">
				<id.ref id="const_reverse_iterator"/>
				<const/>
			</method>
			<method id="rend">
				<id.ref id="reverse_iterator"/>
			</method>
			<method id="rend">
				<id.ref id="const_reverse_iterator"/>
				<const/>
			</method>
		</access>
	</class>
</template>

</namespace>
</namespace>
</header>
</unit>
