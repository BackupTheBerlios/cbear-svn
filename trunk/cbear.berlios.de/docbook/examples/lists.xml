<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="../html.xsl"?>
<section>
	<title>List Examples</title>
	<section>
		<title>Ordered List</title>
		<orderedlist>
			<listitem>
<para>A</para>
			</listitem>
			<listitem>
<para>B</para>
			</listitem>
		</orderedlist>
	</section>
	<section>
		<title>Itemized List</title>
		<itemizedlist>
			<listitem>
<para>A</para>
			</listitem>
			<listitem>
<para>B</para>
			</listitem>
		</itemizedlist>
	</section>
	<itemizedlist>
		<listitem>
			<para>A</para>
			<orderedlist>
				<listitem>
<para>A</para>
				</listitem>
				<listitem>
<para>B</para>
				</listitem>
			</orderedlist>
		</listitem>
		<listitem>
			<para>B</para>
			<orderedlist>
				<listitem>
					<para>C</para>
					<itemizedlist>
						<listitem>
<para>A</para>
						</listitem>
						<listitem>
<para>B</para>
						</listitem>
					</itemizedlist>
				</listitem>
				<listitem>
<para>D</para>
				</listitem>
			</orderedlist>
		</listitem>
	</itemizedlist>
</section>
